# Board
CONFIG_BOARD_XIAOMI=y

# Binder Priority
CONFIG_BINDER_PRIO=y

# Debug
CONFIG_LOG_BUF_SHIFT=22
CONFIG_MI_LOG=y
CONFIG_MI_EXCEPTION_LOG=y
CONFIG_TASK_DELAY_ACCT=y

# Device tree
CONFIG_BUILD_ARM64_DT_OVERLAY=y

# File systems
CONFIG_EXFAT_FS=y

# Fingerprint
CONFIG_INPUT_FINGERPRINT=y

# Hardware ID
CONFIG_MI_HARDWARE_ID=y

# IO Schedulers
CONFIG_CFQ_GROUP_IOSCHED=y

# IR
CONFIG_IR_SPI=y
CONFIG_LIRC=y
CONFIG_RC_CORE=y
CONFIG_RC_DECODERS=y
CONFIG_RC_DEVICES=y

# MIHW
CONFIG_MIHW=y
CONFIG_MILLET_CGROUP=y
CONFIG_MILLET_SIG=y
CONFIG_MILLET_BINDER=y
CONFIG_MILLET_PKG=y
CONFIG_MILLET_BINDER_GKI=y
CONFIG_MILLET_CORE=y
CONFIG_MILLET_HS=y

# NVDIMM
# CONFIG_LIBNVDIMM is not set

# Power supply
# CONFIG_BACKLIGHT_QCOM_SPMI_WLED is not set
CONFIG_LEDS_QTI_FLASH=y
CONFIG_QTI_BATTERY_CHARGER=y

# RCU Subsystem
CONFIG_BLK_CGROUP=y

# Thermal
CONFIG_MI_THERMAL_INTERFACE=y

# Touchscreen
# CONFIG_TOUCHSCREEN_FTS is not set
# CONFIG_TOUCHSCREEN_NT36XXX is not set
# CONFIG_TOUCHSCREEN_ST is not set

# WiFi
CONFIG_ICNSS2=y
# CONFIG_ICNSS2_RESTART_LEVEL_NOTIF is not set
CONFIG_CNSS2=y
CONFIG_QCA_CLD_WLAN=y

# ZRAM
CONFIG_ZRAM_WRITEBACK=y
CONFIG_ZRAM_LZ4P=y
CONFIG_ZRAM_DEF_COMP_LZ4P=y
CONFIG_ZRAM_MULTI_COMP=y
CONFIG_ZRAM_DEDUP=y